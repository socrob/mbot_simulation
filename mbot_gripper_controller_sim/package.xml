<?xml version="1.0"?>
<package>
  <name>mbot_gripper_controller_sim</name>
  <version>0.1.0</version>
  <description>
    Provides with topic and action lib interface to actuate the gripper in simulation
  </description>

  <author email="olima@isr.tecnico.ulisboa.pt">Oscar Lima</author>

  <maintainer email="olima@isr.tecnico.ulisboa.pt">Oscar Lima</maintainer>

  <license>GPLv3</license>
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>actionlib</build_depend>
  <build_depend>control_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>mcr_manipulation_msgs</build_depend>
  
  <run_depend>mcr_manipulation_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>brics_actuator</run_depend>

</package>
